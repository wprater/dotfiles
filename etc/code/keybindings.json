// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "cmd+numpad_add",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "shift+cmd+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "cmd+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "cmd+numpad_subtract",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "shift+cmd+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "cmd+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "cmd+numpad0",
    "command": "editor.action.fontZoomReset"
  },
  {
    "key": "cmd+0",
    "command": "editor.action.fontZoomReset"
  },
  {
    "key": "cmd+ctrl+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+\\",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "cmd+ctrl+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+ctrl+k",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+ctrl+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+\\",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "shift+cmd+[",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "shift+cmd+]",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "cmd+l",
    "command": "expandLineSelection",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+'",
    "command": "esQuotes.transformBetweenSingleDoubleQuotes",
    "when": "editorTextFocus && editorLangId == 'javascript'"
  },
  {
    "key": "shift+ctrl+up",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+ctrl+down",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+down",
    "command": "extension.changeCase.lowerFirst",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+up",
    "command": "editor.emmet.action.incrementNumberByOneTenth",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+f",
    "command": "editor.action.format",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+w",
    "command": "editor.emmet.action.wrapWithAbbreviation",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+c",
    "command": "rest-client.copy-request-as-curl",
    "when": "editorTextFocus && editorLangId == 'http'"
  },
  {
    "key": "alt+cmd+g e",
    "command": "gitlens.showGitExplorer"
  },
  {
    "key": "ctrl+shift+f",
    "command": "azureFunctionsExplorer.focus"
  },
  {
    "key": "alt+cmd+g v",
    "command": "workbench.view.extension.gitlens"
  }
]
